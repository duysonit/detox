{
  "name": "pqaa_detox",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:build": "react-native run-android",
    "ios:build": "react-native run-ios",
    "start": "react-native start",
    "detox:build:ios-debug": "detox build --configuration ios.sim.debug && mkdirp -p e2e/.tmp",
    "detox:e2e:ios-debug": "DETOX_CONFIGURATION=ios.sim.debug DETOX_RECORD_VIDEOS='failing' DETOX_RECORD_LOGS='failing' npx cucumber-js",
    "detox:build:ios-release": "detox build --configuration ios.sim.release && mkdirp -p e2e/.tmp",
    "detox:e2e:ios-release": "DETOX_CONFIGURATION=ios.sim.release DETOX_RECORD_VIDEOS='failing' DETOX_RECORD_LOGS='failing' DETOX_CLEANUP=true npx cucumber-js",
    "detox:e2e:ios-release-headless": "DETOX_CONFIGURATION=ios.sim.release DETOX_RECORD_VIDEOS='failing' DETOX_RECORD_LOGS='failing' DETOX_CLEANUP=true DETOX_HEADLESS=true npx cucumber-js",
    "detox:build:android-debug": "detox build --configuration android.emu.debug && mkdirp -p e2e/.tmp",
    "detox:e2e:android-debug": "DETOX_CONFIGURATION=android.emu.debug DETOX_RECORD_VIDEOS='failing' DETOX_RECORD_LOGS='failing' npx cucumber-js",
    "detox:build:android-release": "detox build --configuration android.emu.release && mkdirp -p e2e/.tmp",
    "detox:e2e:android-release": "DETOX_CONFIGURATION=android.emu.release DETOX_RECORD_VIDEOS='failing' DETOX_RECORD_LOGS='failing' DETOX_CLEANUP=true npx cucumber-js",
    "detox:ios-report-generation": "cd e2e && DEVICE=\"iPhone 14 Pro\" OS=iOS VERSION=14.5 gulp report-generation && cd ..",
    "detox:android-report-generation": "cd e2e && DEVICE=\"Pixel XL\" OS=Android VERSION=10 gulp report-generation && cd ..",
    "detox:report-generation": "cd e2e && gulp report-generation && cd .."
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.22.15",
    "@react-native-community/async-storage": "1.12.1",
    "@react-native-community/datetimepicker": "7.0.0",
    "@react-native-community/eslint-config": "3.2.0",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-picker/picker": "2.4.9",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.4",
    "react-native-asset": "2.1.0",
    "react-native-calendars": "1.1294.0",
    "react-native-config": "^1.5.1",
    "react-native-elements": "3.4.3",
    "react-native-gesture-handler": "2.9.0",
    "react-native-maps": "1.4.0",
    "react-native-reanimated": "3.0.2",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "3.20.0",
    "react-native-splash-screen": "3.3.0",
    "react-native-tabview": "^1.0.0",
    "react-native-vector-icons": "9.2.0",
    "react-navigation": "4.4.4",
    "react-navigation-stack": "2.10.4",
    "react-navigation-tabs": "2.11.2"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@babel/plugin-transform-react-jsx-source": "^7.22.5",
    "@babel/preset-env": "7.20.2",
    "@babel/register": "^7.21.0",
    "@babel/runtime": "7.21.0",
    "@cucumber/cucumber": "^9.0.1",
    "babel-jest": "29.2.1",
    "chai": "^4.3.7",
    "detox": "^20.6.0",
    "detox-getprops": "^0.1.2",
    "eslint": "8.19.0",
    "fs-extra": "^11.1.1",
    "gulp": "^4.0.2",
    "jest": "29.2.1",
    "metro-react-native-babel-preset": "0.73.8",
    "multiple-cucumber-html-reporter": "^3.3.0",
    "prettier": "2.4.1",
    "react-test-renderer": "18.2.0",
    "replace-in-file": "^6.3.5",
    "tempfile": "3.0.0",
    "util": "^0.12.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
